#
# bqcore - Pylons development environment configuration
#
# The %(here)s variable will be replaced with the parent directory of this file
#
# This file is for deployment specific config options -- other configuration
# that is always required for the app is done in the config directory,
# and generally should not be modified by end users.

[DEFAULT]

[server:main]
use = egg:Paste#http
host = %(http_host)s
port = %(http_port)s

[sa_auth]
cookie_secret = 670cbb6c-641d-4089-95a4-7128e2e23d75


[app:main]
use = config:site.cfg
bisque.server=%(server)s
bisque.root=%(rooturl)s
bisque.services_enabled=%(services_enabled)s
bisque.services_disabled=%(services_disabled)s
#bisque.proxyroot= %(proxyroot)s


