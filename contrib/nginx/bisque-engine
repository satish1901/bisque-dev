server {
    listen       8081;
    root <bisque_install_dir>/bqcore/bq/client_service/public;
    charset utf-8;

    location / {
        uwsgi_pass unix://<bisque_install_dir>/tmp/engine.sock; 
        include uwsgi_params;
        uwsgi_param SCRIPT_NAME "";
	      uwsgi_read_timeout 600;
        uwsgi_send_timeout 600;
        uwsgi_buffering off;
    }

    error_page   500 502 503 504  /50x.html;
    location = /50x.html {
        root   html;
    }
}

