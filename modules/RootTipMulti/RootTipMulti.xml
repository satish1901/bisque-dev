<?xml version="1.0" encoding="utf-8"?>
<module name="RootTipMulti" path="./launch.py" type="runtime" >
    
    <tag name="inputs">
        <tag name="input-system" value="image_url"  />
        <tag name="input-system" value="mex_url" />
    </tag>
    
    <tag name="execute_options">
        <tag name="argument_style" value="named" />
    </tag>

    <tag name="module_options" >
        <tag name="version" value="2" />
    </tag>   
    
    <tag name="display_options" >
        <tag name="group" value="application" />
    </tag>
    
    <tag name="interface" type="file" value="public/index.html" />
    <tag name="help"      type="file" value="public/help.html" />
    <tag name="thumbnail" type="file" value="public/thumbnail.png" />   
    <tag name="description" type="string" value="This tracks several root tips in a time series image." />          
    
</module>
